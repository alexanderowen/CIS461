/* inherit.qk
 *
 * Demonstrates user defined classes, featuring method overwritting and inheritance
 * 
 * Output:
 * Called B.foo
 * Called A.bar
 */
class A() {
    def foo() {
        "Called A.foo\n".PRINT();
    }

    def bar() {
        "Called A.bar\n".PRINT();
    }
}

class B() extends A {
    def foo() {
        "Called B.foo\n".PRINT();
    }
}

b = B();
b.foo(); //Overwritten method
b.bar(); //Inherited method
